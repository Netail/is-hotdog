	"description": "Is this a hotdog?",
	"license": "MIT",
	"files": ["dist"],
	"main": "dist/index.js",
	"packageManager": "yarn@1.22.22",
	"bin": {
		"is-hotdog": "dist/cli.js"
	},
	"scripts": {
